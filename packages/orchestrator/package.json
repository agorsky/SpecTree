{
  "name": "@spectree/orchestrator",
  "version": "0.2.0",
  "description": "Parallel AI Agent Orchestrator CLI for SpecTree",
  "type": "module",
  "bin": {
    "spectree-agent": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@github/copilot-sdk": "^0.1.0",
    "@spectree/shared": "workspace:^",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "inquirer": "^10.2.2",
    "open": "^11.0.0",
    "ora": "^8.1.1",
    "simple-git": "^3.27.0",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.0",
    "@types/ws": "^8.5.13",
    "@vitest/coverage-v8": "^2.1.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "spectree",
    "copilot",
    "ai",
    "orchestrator",
    "cli",
    "parallel-agents"
  ],
  "license": "MIT"
}
